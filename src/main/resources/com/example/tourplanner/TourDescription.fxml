<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.ToolBar?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.tourplanner.view.controller.TourDescriptionController">
   <children>
      <ToolBar prefHeight="40.0" prefWidth="200.0">
        <items>
            <Label prefHeight="25.0" prefWidth="48.0" text="Title: ">
               <padding>
                  <Insets bottom="4.3" left="8.7" right="8.7" top="4.3" />
               </padding>
            </Label>
            <TextField fx:id="textField" prefHeight="25.0" prefWidth="392.0" />
        </items>
      </ToolBar>
      <HBox>
         <children>
            <Button mnemonicParsing="false" text="Route" />
            <Button mnemonicParsing="false" text="Description" />
            <Button maxWidth="1.7976931348623157E308" mnemonicParsing="false" HBox.hgrow="ALWAYS" />
         </children>
      </HBox>
      <TextArea />
   </children>
</VBox>
