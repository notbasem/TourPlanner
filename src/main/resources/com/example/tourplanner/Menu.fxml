<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox prefHeight="66.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.tourplanner.view.controller.MenuController">
   <children>
      <Label maxWidth="1.7976931348623157E308" text="Tour-Planner">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
         <VBox.margin>
            <Insets />
         </VBox.margin>
         <padding>
            <Insets bottom="4.3" left="8.7" right="8.7" top="4.3" />
         </padding>
      </Label>
      <MenuBar>
        <menus>
          <Menu mnemonicParsing="false" text="File">
            <items>
               <MenuItem fx:id="exportButton" mnemonicParsing="false" onAction="#onExport" text="Export" />
            </items>
          </Menu>
          <Menu mnemonicParsing="false" text="Edit" />
            <Menu mnemonicParsing="false" text="Options" />
          <Menu mnemonicParsing="false" text="Help">
            <items>
              <MenuItem mnemonicParsing="false" text="About" />
            </items>
          </Menu>
        </menus>
      </MenuBar>

      <HBox maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1">
         <children>
            <TextField fx:id="searchTextField" maxWidth="1.7976931348623157E308" promptText="Search" HBox.hgrow="ALWAYS" />
         </children>
      </HBox>
   </children>
</VBox>
